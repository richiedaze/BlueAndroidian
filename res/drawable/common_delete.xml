<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_checked="true" android:state_focused="true"
          	android:drawable="@drawable/common_delete_on_selected" />
    <item android:state_checked="false" android:state_focused="true"
          	android:drawable="@drawable/common_delete_off_selected" />
    <item android:state_checked="true" android:state_selected="true"
          	android:drawable="@drawable/common_delete_on_selected" />
    <item android:state_checked="false" android:state_selected="true"
          	android:drawable="@drawable/common_delete_off_selected" />
    <item android:state_checked="false" 
    		android:drawable="@drawable/common_delete_off" />
    <item android:state_checked="true" 
            android:drawable="@drawable/common_delete_on" />
</selector>

